
@charset "utf-8";
#PmtContainer {
	/* global defaults */
	body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,textarea,p,blockquote,th,td,legend{margin:0;padding:0; box-sizing:border-box;}
	fieldset,img,abbr,acronym{border:none}
	fieldset{display:block}
	ol,ul{list-style:none outside}
	a{text-decoration:none;}
	h1,h2,h3,h4,h5,h6,address,caption,cite,code,dfn,em,th,var{font-size:100%;font-weight:normal}
	em{font-style:normal;}
	strong{font-weight:600;}
	hr,legend,caption,hr{visibility:hidden;overflow:hidden;position:absolute;top:0;left:0;width:0;height:0;margin:0;padding:0;font-size:0;line-height:0;text-indent:-1000em}
	input[type=text],
	input[type=password]{vertical-align:top; background-color:#fff; border:1px solid #a6acb1; border-radius:3px; padding:0 12px; height:30px; line-height:28px; font-size:13px; color:#555;}
	/* input[type=text].datepicker{background:#fff url("../../images/common/ico_datepicker.png") no-repeat right 10px center; padding:0 35px 0 12px;} */
	/* kovex */
	input[type=text].datepicker{background:#fff no-repeat right 10px center; padding:0 35px 0 12px;}
	input[type=email]{background:transparent; border:none; outline:0;}

	label{user-select:none; -ms-user-select:none; -moz-user-select:none; -webkit-user-select:none;}
	/* checkbox */
	input[type=checkbox] {width:0; height:0; -webkit-transform:scale(0, 0); -moz-appearance:none;}
	/* input[type=checkbox] + label {background:url("../images/img/check_off.png") no-repeat 0 center; display:inline-block; padding-left:20px; padding-right:10px; cursor:pointer; color:#222; background-size:16px auto;}
	input[type=checkbox]:checked + label {background:url("../images/img/check_on.png") no-repeat 0 center; cursor:pointer; background-size:16px auto;} */
	input[type=checkbox] + label {background: no-repeat 0 center; display:inline-block; padding-left:20px; padding-right:10px; cursor:pointer; color:#222; background-size:16px auto; font-size:16px;}
	input[type=checkbox]:checked + label {background: no-repeat 0 center; cursor:pointer; background-size:16px auto;}

	/* radio button */
	input[type=radio] {width:0; height:0; -webkit-transform: scale(0, 0); -moz-appearance:none;}
	/* input[type=radio] + label {background:url("../images/img/radio_off.png") no-repeat 0 center; display:inline-block; padding-left:22px; padding-right:10px; cursor:pointer; font-size:13px; color:#222; height:20px;} */
	input[type=radio] + label {background: no-repeat 0 center; display:inline-block; padding-left:22px; padding-right:10px; cursor:pointer; font-size:13px; color:#222; height:20px;}
	/* input[type=radio]:checked + label {background:url("../images/img/radio_on.png") no-repeat 0 center; cursor:pointer;} */
	input[type=radio]:checked + label {background: no-repeat 0 center; cursor:pointer;}

	/* select */
	/* select{font-weight:400!important; border:1px solid #a6acb1; border-radius:3px; -webkit-appearance:none; -moz-appearance:none; appearance:none; background:#fff url("../images/img/arr_right.png") no-repeat right 10px center; padding-left:10px;} */
	select{font-weight:400!important; border:1px solid #a6acb1; border-radius:3px; -webkit-appearance:none; -moz-appearance:none; appearance:none; background:#fff no-repeat right 10px center; padding-left:10px;}
	select::-ms-expand{display:none;}

	/* input file */
	.file{width:100%; position:relative; overflow:hidden;}
	.file>.file_wrap{width:107px; height:38px; position:relative; border:none; vertical-align:top; cursor:pointer; overflow:hidden; display:inline-block;}
	.file>.file_wrap>input[type=file]{position:absolute; top:0; left:0;  width:107px; height:38px; opacity:0; cursor:pointer; z-index:3;}
	.file .text{position:absolute; top:0; left:0; width:105px; height:38px; line-height:38px; text-align:center; display:inline-block; border-radius:3px;}

	textarea{width:100%; background-color:#fff; border:1px solid #c0c5c8; border-radius:2px; padding:12px; height:150px; line-height:20px; font-size:15px; color:#777; resize:none; }
	a,
	button{display:inline-block; vertical-align:top;}
	/* button,
	button:focus,
	button:active{padding:0;} */
	button > span.click-clear{position:relative;}
	button{border:0; outline:none; cursor:pointer; background-color:transparent;}

