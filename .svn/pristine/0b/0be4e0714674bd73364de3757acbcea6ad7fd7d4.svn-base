        display:block;
        top:-30px;
        right:0;
        color:#212121;
    }
    .Exchange, .DepositWithdraw {
        width:1440px !important;
        margin:0 auto;
        background:none !important;
        padding:0 !important;
    }
    .TdiVue, .SmartSignal, .Customers, .MyPage {
        width:1200px !important;
        margin:0 auto;
        background:none !important;
        padding:0 !important;
    }
