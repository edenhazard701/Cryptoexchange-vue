@charset 'utf-8';
@import url('https://fonts.googleapis.com/css?family=Roboto:100,300,400,500');

@import "./css_chart.scss";
@import "./swiper.css";

/* IE용 Noto Sans font 적용 */
@font-face {
    font-family:'Noto Sans KR';
    font-style:normal;
    font-weight:100;
    src: url('//fonts.gstatic.com/ea/notosanskr/v2/NotoSansKR-Thin.woff2') format('woff2'),
        url('//fonts.gstatic.com/ea/notosanskr/v2/NotoSansKR-Thin.woff') format('woff'),
        url('//fonts.gstatic.com/ea/notosanskr/v2/NotoSansKR-Thin.otf') format('opentype')
}
@font-face {
    font-family:'Noto Sans KR';
    font-style:normal;
    font-weight:300;
    src: url('//fonts.gstatic.com/ea/notosanskr/v2/NotoSansKR-Light.woff2') format('woff2'),
        url('//fonts.gstatic.com/ea/notosanskr/v2/NotoSansKR-Light.woff') format('woff'),
        url('//fonts.gstatic.com/ea/notosanskr/v2/NotoSansKR-Light.otf') format('opentype')
}
@font-face {
    font-family:'Noto Sans KR';
    font-style:normal;
    font-weight:400;
    src: url('//fonts.gstatic.com/ea/notosanskr/v2/NotoSansKR-Regular.woff2') format('woff2'),
        url('//fonts.gstatic.com/ea/notosanskr/v2/NotoSansKR-Regular.woff') format('woff'),
        url('//fonts.gstatic.com/ea/notosanskr/v2/NotoSansKR-Regular.otf') format('opentype')
}
@font-face {
    font-family:'Noto Sans KR';
    font-style:normal;
    font-weight:500;
    src: url('//fonts.gstatic.com/ea/notosanskr/v2/NotoSansKR-Medium.woff2') format('woff2'),
        url('//fonts.gstatic.com/ea/notosanskr/v2/NotoSansKR-Medium.woff') format('woff'),
        url('//fonts.gstatic.com/ea/notosanskr/v2/NotoSansKR-Medium.otf') format('opentype')
}

button, textarea, select, input { font-family: Roboto, 'Noto Sans KR', sans-serif; }
body {
    margin: 0px;
    padding: 0px;
    font-family: Roboto, 'Noto Sans KR', sans-serif;
    font-size: 14px;
    color:#212121;
    -webkit-font-smoothing: antialiased;
}
a {
    text-decoration: none;
    color: inherit;
}
:focus { box-shadow:none !important; }
* {
    margin: 0;
    padding: 0;
    box-sizing: border-box
}
dl, ul, li, dt, dd {
    list-style:none;
    margin:0;
    padding:0;
}

/* input box */
.el-input .el-input__inner {
    height:30px;
    font-family:Roboto, 'Noto Sans KR', sans-serif;
    font-size:14px;
    font-weight:400;
    color:#212121;
    line-height:normal;
    border:1px solid #e1dfe1;
    border-radius:0;
    padding-left:10px;
}
.el-input input:hover { border:1px solid #C0C4CC !important; }
.el-input input:focus { border:1px solid #409EFF !important; }
.el-input input::placeholder { text-align:left; color:#bdbdbd; }
.el-input input::-webkit-input-placeholder { text-align:left; color:#bdbdbd; }
.el-input input:-ms-input-placeholder { text-align:left !important; color:#bdbdbd !important; }
.el-input input::-ms-input-placeholder { text-align:left; color:#bdbdbd; }
.el-textarea textarea::placeholder { color:#bdbdbd; }
.el-textarea textarea::-webkit-input-placeholder { color:#bdbdbd; }
.el-input.is-disabled .el-input__inner { background-color:#fafafa !important; }
.el-textarea textarea {
    font-size:14px;
    font-family:Roboto, 'Noto Sans KR', sans-serif;
    color:#616161;
    border:1px solid #dcdfe6;
    border-radius:0;
}

/* radio */
.el-radio__input.is-checked .el-radio__inner {
    width:20px;
    height:20px;
    background:#00e2aa !important;
    border-color:#00e2aa !important;
}
.el-radio__inner {
    width:20px !important;
    height:20px !important;
    border:2px #e4e4e4 solid !important;
}
.el-radio__inner::after {
    width:10px !important;
    height:10px !important;
    background-color:#fff !important;
}
.el-radio__label {
    font-size:12px !important;
    color:#616161 !important;
    padding-left:10px;
}
.el-radio__input.is-checked+.el-radio__label {
    font-size:12px;
    color:#000 !important;
}

/* checkbox */
.el-checkbox__inner {
    width:20px !important;
    height:20px !important;
    border:2px #e4e4e4 solid !important;
    border-radius:3px;
}
.el-checkbox__input.is-checked .el-checkbox__inner,
.el-checkbox__input.is-indeterminate .el-checkbox__inner {
    border-color:#00e2aa !important;
    background-color:#00e2aa !important;
}
.el-checkbox__input.is-indeterminate .el-checkbox__inner::before { top:7px !important; }
.el-checkbox__label {
    font-size:14px !important;
    font-weight:400;
    padding-left:8px !important;
    color:#616161 !important;
}
.el-checkbox__input.is-checked+.el-checkbox__label { color:#4b4b4b !important; }
.el-checkbox__inner::after {
    top:5px !important;
    left:3px !important;
    border:0 !important;
    width:10px !important;
    transform:none !important;
}
.el-checkbox__input.is-checked .el-checkbox__inner::after { background:url('../images/ico_check.png') no-repeat; }
.el-checkbox__input.is-disabled .el-checkbox__inner {
    background-color:#eee !important;
    border-color:#e0e0e0 !important;
}
.el-checkbox__input.is-disabled .el-checkbox__inner::after { transform:none !important; }
.el-checkbox__input.is-disabled+span.el-checkbox__label { color:#dbdbdb !important; }
.el-checkbox__input.is-disabled.is-checked .el-checkbox__inner::after { background:url('../images/ico_check_disabled.png') no-repeat; }

/* select box */
.el-select > .el-input {
    display:inline-block !important;
    line-height:normal;
}
.el-select .el-input__inner {
    border:1px #e0e0e0 solid;
    border-radius:0;
}
.el-select .el-input__icon { width:10px; }
.el-select .el-input__suffix { right:15px !important; }
.el-select .el-input__suffix-inner {
    display:block;
    height:100%;
}
.el-select .el-icon-arrow-up:before {
    content:'' !important;
    display:block;
    width:100%;
    height:100%;
    background:url('../images/arrow_down.png') center no-repeat;
    transform:rotateZ(180deg);
}
.el-select .el-input__icon::after { display:none; }

/* select dropdown */
.el-select-dropdown,
.el-dropdown-menu,
.el-autocomplete-suggestion,
.el-picker-panel {
    margin-top:0 !important;
    border-radius:0 !important;
    box-shadow:0 2px 2px rgba(0,0,0,0.05) !important;
    z-index:1000 !important;
}
.v-modal { z-index:2000 !important; }
.v-modal + .el-select-dropdown,
.el-select-dropdown + .el-select-dropdown,
.v-modal + .el-dropdown-menu,
.v-modal + .el-autocomplete-suggestion,
.v-modal + .el-picker-panel,
.el-picker-panel + .el-picker-panel { z-index:20000 !important; }
.el-select-dropdown.local-popper { z-index:2001 !important; }
.el-dropdown-menu { padding: 5px 0 !important; }
.el-dropdown-menu .popper__arrow,
.el-select-dropdown .popper__arrow,
.el-autocomplete-suggestion .popper__arrow,
.el-picker-panel .popper__arrow { display:none !important; }
.el-select-dropdown__item.selected,
.el-date-table td.today span,
.el-date-table td.available:hover { color:#304ffe !important; }
.el-date-table td.end-date span,
.el-date-table td.start-date span { background:#3f51b5 !important; }
.el-date-table td.today.end-date span,
.el-date-table td.today.start-date span { color:#fff !important; }
.el-dropdown-menu__item,
.el-select-dropdown__item {
    font-family: Dotum;
    color:#606266 !important;
    font-size:12px !important;
    padding:0 15px !important;
}
.el-dropdown-menu__item:hover { background:#f5f7fa !important; }

/* page loading */
.pageLoading {
    position:fixed;
    display:flex;
    top:0;
    left:0;
    width:100%;
    height:100%;
    font-size:16px;
    color:#000;
    background:rgba(255,255,255,0.2);
    justify-content:center;
    align-items:center;
    z-index:100000;
}
.pageLoading .spin {
    position:relative;
    display:block;
    width:80px;
    height:80px;
    background:url('../images/m_paging_loading.png') no-repeat;
    background-size:100%;
    animation:spin 3s infinite linear;
}

/*loading*/
.el-table__append-wrapper {
    top:0;
    left:0;
    width:100%;
    height:100%;
    pointer-events:none;
}
.StockWatch .el-table__append-wrapper { height:auto; }
.OrderHistory .el-table__append-wrapper { height:auto; }
.PcContainer.full { height:100%; }
.v-modal,
.el-message-box__wrapper {
    transition:none !important;
    animation:none !important;
}
.PcContainer {
    background:#e8ebee;
    min-height:100%;

    .el-dialog__wrapper,
    .dialog-fade-enter-active,
    .dialog-fade-leave-active {
        animation:none !important;
        transition:none !important;
    }

    .Header {
        position:fixed;
        top:0;
        left:0;
        width:100%;
        height:auto !important;
        min-height:60px;
        padding: 0 !important;
        background-color:#3f51b5;
        z-index:2000;
    }
    .Header.isScroll { box-shadow:0 2px 2px rgba(0,0,0,0.2); }
    .Header .el-menu {
        background:none;
        border:0;
    }
    .Header > .el-container {
        width:100%;
        max-width: 1440px;
        margin:0 auto;
    }
    .Header + .el-container {
        width:1200px;
        background:#fff;
        margin:0 auto;
        margin-top:120px;
        min-height:720px;
        padding:40px;
    }
    .Header + .el-container.PageCommon {
        width:100%;
        height:100%;
        margin-top:0;
        padding:0;
    }
    .Header + .el-container > .contents {
        position:relative;
        width:100%;
    }
    .Header + .el-container > .el-aside {
        width: 220px !important;
        background:#fff;
        margin-right:20px;
    }
    .Header + .el-container > .el-aside + .el-main {
        background:#fff;
        padding:40px !important;
    }
    .Header + .el-container > .el-aside .aside-title {
        font-size:30px;
        color:#212121;
        font-weight:500;
        line-height:1;
        padding:40px 20px 20px;
    }
    .Header + .el-container > .el-aside > .el-menu {
        border-right:none;
        border-top:1px #f0f2f7 solid;
    }
    .Header + .el-container > .el-aside > .el-menu > .el-menu-item {
        display:flex;
        height:50px;
        border-bottom:1px #f0f2f7 solid;
        align-items:center;
    }
    .Header + .el-container > .el-aside > .el-menu > .el-menu-item::after {
        content:'';
        position:absolute;
        top:calc(50% - 5px);
        right:10px;
        width:6px;
        height:10px;
        background:url('../images/paging_next.png') no-repeat;
        opacity:0.7;
    }
    .Header + .el-container > .el-aside > .el-menu > .el-menu-item.is-active,
    .Header + .el-container > .el-aside > .el-menu > .el-submenu.is-active.is-opened > .el-submenu__title {
        color:#fff;
        background:#3f51b5 !important;
    }
    .Header + .el-container > .el-aside > .el-menu > .el-menu-item.is-active::after {
        background-image:url('../images/main_notice_more.png');
        opacity:1;
    }
    .Header + .el-container > .el-aside > .el-menu > .el-menu-item:hover,
    .Header + .el-container > .el-aside > .el-menu > .el-submenu > .el-submenu__title:hover { background:#f4f6ff; }
    .Header + .el-container > .el-aside > .el-menu > .el-menu-item:hover::before,
    .Header + .el-container > .el-aside > .el-menu > .el-submenu > .el-submenu__title:hover::before {
        content:'';
        position:absolute;
        top:0;
        left:0;
        width:4px;
        height:100%;
        background:#3f51b5;
    }
    .Header + .el-container > .el-aside > .el-menu > .el-menu-item:not(.is-active):hover::after {
        opacity:1;
        background-image:url('../images/ico_lnb_blue.png');
    }
    .Header + .el-container > .el-aside > .el-menu > .el-menu-item:focus { background:none; }
    .Header + .el-container > .el-aside > .el-menu > .el-menu-item.kakao { padding:0 !important; }
    .Header + .el-container > .el-aside > .el-menu > .el-menu-item.kakao > a {
        position:relative;
        display:block;
        padding:0 20px;
    }
    .Header + .el-container > .el-aside > .el-menu > .el-submenu { border-bottom:1px #f0f2f7 solid; }
    .Header + .el-container > .el-aside > .el-menu > .el-submenu > .el-submenu__title { height:50px; }
    .Header + .el-container > .el-aside > .el-menu > .el-submenu .el-icon-arrow-down {
        top:calc(50% - 3px);
        right:10px;
        width:10px;
        height:6px;
        margin-top:0;
    }
    .Header + .el-container > .el-aside > .el-menu > .el-submenu > .el-submenu__title .el-icon-arrow-down::before {
        content:'';
        display:block;
        width:10px;
        height:6px;
        opacity:0.7;
        background:url('../images/arrow_down.png') no-repeat;
    }
    .Header + .el-container > .el-aside > .el-menu > .el-submenu.is-active.is-opened > .el-submenu__title .el-icon-arrow-down::before {
        opacity:1;
        background-image:url('../images/header_arrow_down.png');
    }
    .Header + .el-container > .el-aside > .el-menu > .el-submenu > .el-submenu__title:hover .el-icon-arrow-down::before {
        opacity:1;
        background-image:url('../images/header_arrow_down_blue.png');
    }
    .Header + .el-container > .el-aside > .el-menu > .el-submenu .el-menu-item {
        height:50px;
        color:#9e9e9e;
        font-weight:300;
        border-top:1px #dfe4ee solid;
        background:#f9f9f9;
        padding:0 20px !important;
        margin-bottom:-1px;
    }
    .Header + .el-container > .el-aside > .el-menu > .el-submenu .el-menu-item.is-active {
        color:#304ffe;
        font-weight:400;
    }
    .Header + .el-container > .el-aside > .el-menu > .el-submenu .el-menu-item:last-child { border-bottom:1px #dfe4ee solid; }
    .Header + .el-container > .contents > .el-menu {
        height:50px;
        border-bottom:2px #444546 solid !important;
        margin-bottom:40px;
    }
    .Header + .el-container > .contents > .el-menu::after { display:none; }
    .Header + .el-container > .contents > .el-menu > .el-menu-item {
        width:20%;
        height:48px;
        font-size:20px;
        font-weight:300;
        color:#616161;
        line-height:46px;
        text-align:center;
        border-top:1px #C6C6C6 solid;
        border-left:1px #C6C6C6 solid;
        border-bottom:0 !important;
        background:#f9f9f9;
        transition:none;
    }
    .Header + .el-container > .contents > .el-menu > .el-menu-item:last-child { border-right:1px #c6c6c6 solid; }
    .Header + .el-container > .contents > .el-menu > .el-menu-item.is-active {
        height:50px;
        font-weight:400;
        color:#212121;
        line-height:46px;
        border:2px #444546 solid;
        background:#fff;
    }
    .Header + .el-container > .contents > .el-menu > .el-menu-item.is-active + .el-menu-item { border-left:1px transparent solid; }
    .NaviBar {
        position:relative;
        height:40px;
        background:#fff;
        border-bottom:1px #d5dadb solid;
    }
    .NaviBar .el-breadcrumb {
        position:relative;
        width:1440px;
        margin:0 auto;
        padding-left:10px;
    }
    .NaviBar .el-breadcrumb .is-link {
        font-weight:normal;
        color:#616161;
    }
    .NaviBar .el-breadcrumb .el-breadcrumb__separator[class*=icon] {
        font-size:11px;
        margin:0 5px;
        color:#bdbdbd;
    }
    .NaviBar .el-breadcrumb .el-breadcrumb__item > .el-breadcrumb__inner {
        font-size:12px;
        color:#616161;
        line-height:40px;
    }
    .NaviBar .el-breadcrumb .el-breadcrumb__item:last-child > .el-breadcrumb__inner {
        font-weight:500;
        color:#304ffe;
    }
    .pageTitle {
