/* 20191105 추가페이지 */
.MyRestriction, .modal {
  .alignR {
    text-align: right !important;
  }
  .alignC {
    text-align: center !important;
  }
  .alignL {
    text-align: left !important;
  }
  // input
  .input {
    height: 30px;
    padding: 0 10px;
    border: 1px solid #e0e0e0;
  }
  // button
  .btns {
    margin: 20px 0 30px;
    &:first-child {
      margin-top: 0;
    }
    &:last-child {
      margin-bottom: 0;
    }
    .button {
      min-width: 100px;
    }
  }
  .button {
    border: 1px solid #ddd;
    &.el-button {
      &:focus,
      &:hover {
        border: 1px solid #ddd;
        background: #fff;
        color: #606266;
      }
    }
    &[class*=bg] {
      color: #fff;
      &:focus,
      &:hover {
        color: #fff;
      }
    }
    &.bg01 {
      background-color: #3f51b5;
      &:focus,
      &:hover {
        background-color: #3f51b5;
      }
    }
    &.sizeS {
      height: 30px;
    }
  }
  // table
  .table {
    margin-top: 20px;
    table {
      width: 100%;
      border-top: 2px solid #000;
      border-spacing: 0;
      border-collapse: collapse;
    }
    th,
    td {
      height: 40px;
      padding-top: 5px;
      padding-left: 20px;
      padding-bottom: 5px;
      border: 1px solid #ebeef5;
      border-width: 1px 0;
      text-align: left;
    }
    th {
      font-size: 14px;
      color: #333;
      font-weight: 400;
    }
    tbody th {
      background-color: #f9f9f9;
    }
    td {
      word-break: break-all;
    }
  }
  .table-list.el-table--border {
    border: 0;
    &:after {
      display: none;
    }
    th,
    td {
      text-align: center;
    }
    th {
      border-right: 0;
    }
  }
}
