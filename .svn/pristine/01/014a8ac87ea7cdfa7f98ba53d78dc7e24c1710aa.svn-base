{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
    "stage-2"
  ],
  "plugins": [
    "transform-runtime",
    "syntax-dynamic-import",
    ["transform-es2015-template-literals", {
      "loose": true,
      "spec": true
    }]
  ],
  "comments": true,
  "env": {
    "test": {
      "plugins": [ "istanbul" ]
    }
  }
}
